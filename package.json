  "devDependencies": {
    "browser-sync": "^2.18.8",
    "del": "^2.2.0",
    "eslint": "^3.12.0",
    "eslint-config-google": "^0.7.1",
    "eslint-plugin-html": "^1.7.0",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-if": "^2.0.0",
    "gulp-imagemin": "^3.1.0",
    "gulp-load-plugins": "^1.5.0",
